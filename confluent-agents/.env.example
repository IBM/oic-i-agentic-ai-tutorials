# Confluent Kafka Configuration
# Copy this file to .env and fill in your actual values

# Kafka Cluster Configuration
BOOTSTRAP_SERVERS=pkc-xxxxx.region.provider.confluent.cloud:9092
KAFKA_API_KEY=your-kafka-api-key-here
KAFKA_API_SECRET=your-kafka-api-secret-here

# Topic Configuration
TOPIC_NAME=inventory.transactions

# ksqlDB Configuration
KSQLDB_ENDPOINT=https://pksqlc-xxxxx.region.provider.confluent.cloud:443
KSQLDB_API_KEY=your-ksqldb-api-key-here
KSQLDB_API_SECRET=your-ksqldb-api-secret-here